{
  "name": "@teemukoivisto.xyz/vite-plugin-dynamic-templates",
  "version": "0.0.1",
  "homepage": "https://github.com/TeemuKoivisto/vite-plugin-dynamic-templates/README.md",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "format": "prettier --write \"*.+(js|json|yml|yaml|ts|md|graphql|mdx)\" src/",
    "lint": "eslint --cache --ext .js,.ts, ./src ./types",
    "lint:fix": "eslint --fix --ext .js,.ts, ./src ./types",
    "watch": "rollup -cw"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.7.3",
    "@types/shelljs": "^0.8.11",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "shelljs": "^0.8.5"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  }
}
